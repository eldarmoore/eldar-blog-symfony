{% extends 'base.html.twig' %}

{% block title %} Welcome to blog Page{% endblock %}

{% block body %}
    <h1>Welcome to {{ name }}</h1>
    <p>Please visit <i style="color: #0088CC">"<a href="{{ url }}">{{ url }}</a>"</i> for more information.</p>
    <div>
        {% for note in notes %}
            <h2>{{ note.name }} </h2>
            <p>{{ note.quick_content }}</p>
        {% endfor %}
    </div>
{% endblock %}
